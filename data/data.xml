<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="patient_test" model="hospital.patient">
            <field name="name">test patient</field>
            <field name="gender">male</field>
            <field name="age">24</field>
            <field name="email">test@gmail.com</field>
            <field name="address">test address</field>
            <field name="disease">no disease</field>
            <field name="note">no</field>
        </record>
        <record id="doctor_test" model="hospital.doctor">
            <field name="name">test doctor</field>
            <field name="gender">male</field>
            <field name="age">50</field>
            <field name="address">test</field>
        </record>
        <record id="appointment_test" model="hospital.appointment">
            <field name="patient_id" ref="patient_test"/>
            <field name="doctor_id" ref="doctor_test"/>
        </record>

        <record id="lab_test" model="hospital.lab">
            <field name="lab_name">test lab</field>
        </record>

    </data>
</odoo>