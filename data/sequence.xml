<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">OT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_doctor" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor</field>
            <field name="prefix">HD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_Lab" model="ir.sequence">
            <field name="name">Lab Sequence</field>
            <field name="code">hospital.lab</field>
            <field name="prefix">L</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_Lab_records" model="ir.sequence">
            <field name="name">Lab Record Sequence</field>
            <field name="code">hospital.lab.record</field>
            <field name="prefix">LR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_room" model="ir.sequence">
            <field name="name">Room Sequence</field>
            <field name="code">hospital.room</field>
            <field name="prefix">R</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_bill" model="ir.sequence">
            <field name="name">Bill Sequence</field>
            <field name="code">hospital.bill</field>
            <field name="prefix">B</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>

</odoo>